#include "Modules/Telemetry/payload.h"

void DeployParachute(void){
	uint8_t data = 0xFF;

	if((abs((int)MPU6050.KalmanAngleX))>45){
		HAL_UART_Transmit(&huart2, &data, 1, 100);
	}
	else if((abs((int)MPU6050.KalmanAngleY))>45){
		HAL_UART_Transmit(&huart2, &data, 1, 100);
	}
}
